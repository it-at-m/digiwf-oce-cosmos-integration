{
  "name": "Parkausweisdaten an Druckstraße übergeben",
  "id": "de.muenchen.digiwf.druckstrasse.integration.DatenAnDruckstrasse",
  "version": 1,
  "description": "Templatexxx",
  "appliesTo": [
    "bpmn:CallActivity"
  ],
  "properties": [
    {
      "label": "Baustein",
      "type": "String",
      "editable": false,
      "value": "GenericDruckBeauftragen",
      "binding": {
        "type": "property",
        "name": "calledElement"
      }
    },
    {
      "label": "Event Topic",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_topic_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Event Message",
      "type": "String",
      "editable": false,
      "value": "genericEvent",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_message_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
      "label": "Type Header",
      "type": "String",
      "editable": false,
      "value": "requestPrint",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "app_type_name"
      },
      "constraints": {
        "notEmpty": true
      }
    },
    {
	  "label": "Versandart",
	  "description": "Versandart des Dokuments (POSTVERSAND, DIENSTVERSAND) eingeben.",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "BewohnerVersandart"
      },
      "constraints": {
        "notEmpty": true
      }
    },
	    {
	  "label": "Abholort",
	  "description": "Abholort des Druckdokuments nur bei Versandart DIENSTVERSAND eingeben.",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "AbholortDienstversand"
      },
      "constraints": {
        "notEmpty": true
      }
    },
		    {
	  "label": "Druckdaten",
	  "description": "Daten, die an die Druckstrasse übermittelt werden.",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "druckDaten"
      },
      "constraints": {
        "notEmpty": true
      }
    },
		    {
	  "label": "ID der Druckvorlage",
	  "description": "Druckvorlage wird übermittelt",
      "type": "String",
      "value": "",
      "binding": {
        "type": "camunda:in",
        "expression": true,
        "target": "parkausweisVorlage"
      },
      "constraints": {
        "notEmpty": true
      }
    }
  ],
  "entriesVisible": {
    "_all": false
  }
}